


{
  "manifest_version": 2,
  "name": "Leads  tracker",
  "version": "1.0",
  "description": "This saves any leads of importance",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.svg"
  },
  "icons": {
    "48": "icon.svg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]
}
